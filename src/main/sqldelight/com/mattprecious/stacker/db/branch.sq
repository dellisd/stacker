CREATE TABLE branch (
	name TEXT NOT NULL PRIMARY KEY,
	parent TEXT,
	FOREIGN KEY(parent) REFERENCES branch(name) ON DELETE CASCADE ON UPDATE CASCADE
);

insert:
INSERT INTO branch
VALUES (?, ?);

remove:
DELETE FROM branch
WHERE name = ?;

rename:
UPDATE branch
SET name = :newName
WHERE name = :oldName;

selectAll:
SELECT *
FROM branch;

contains:
SELECT COUNT(*) > 0
FROM branch
WHERE name = ?;
